{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react@18.2.0",
    "strict": true,
    "types": [
      "https://esm.sh/@types/react@18.2.45/index.d.ts",
      "https://esm.sh/@types/react-dom@18.2.18/index.d.ts"
    ]
  },
  "imports": {
    "react": "https://esm.sh/react@18.2.0",
    "react/": "https://esm.sh/react@18.2.0/",
    "react-dom": "https://esm.sh/react-dom@18.2.0",
    "react-dom/": "https://esm.sh/react-dom@18.2.0/",
    "react/jsx-runtime": "https://esm.sh/react@18.2.0/jsx-runtime"
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-write --allow-env --allow-run server.ts",
    "test": "deno run --allow-read --allow-write --allow-env tests/phase1.ts",
    "scan": "deno run --allow-read --allow-write --allow-env -c 'import {quickScan} from \"./core/scanner.ts\"; console.log(await quickScan());'"
  }
}